#-*- coding:utf-8 -*-
import xlrd
import xlwt
from sklearn import svm
import numpy as np  
import pandas as pd

moData = xlrd.open_workbook(r'C:/Users/tony.zhou/Downloads/OpenMO1.xlsx')
moTable = moData.sheets()[0]
X = list()
y = []
for i in range(moTable.nrows):
	if i == 0:
		pass
	else:
		X.append(moTable.row_values(i)[1:5])
		y += moTable.row_values(i)[5:6]
clf = svm.SVC() 
clf.fit(X,y)

mo2Data = xlrd.open_workbook(r'C:/Users/tony.zhou/Downloads/OpenMO2.xlsx')
mo2Table = mo2Data.sheets()[0]
X2 = list()
y2 = list()
print(mo2Table.nrows)
for j in range(mo2Table.nrows):
	if j == 0:
		pass
	else:		
		X2 = [mo2Table.row_values(j)[1:5]]	
		y2.append(clf.predict(X2))
dataFrame = pd.DataFrame({'status':y2})
dataFrame.to_csv(r'C:\Users\tony.zhou\Downloads\test.csv',index=False,sep=',')
